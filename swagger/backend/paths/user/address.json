{
    "swagger": "2.0",
    "paths": {
        "/users/receive-addresses/search": {
            "post": {
                "tags": [
                    "User"
                ],
                "summary": "收货地址查询",
                "parameters": [
                    {
                        "$ref": "#/definitions/Token"
                    },
                    {
                        "in": "body",
                        "name": "body",
                        "required": true,
                        "description": "查询条件",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "paging": {
                                    "$ref": "definitions/search/Paging.json"
                                },
                                "condition": {
                                    "type": "object",
                                    "description": "查询条件",
                                    "properties": {
                                        "recieveName": {
                                            "type": "string",
                                            "description": "收货人姓名"
                                        },
                                        "cellphone": {
                                            "type": "string",
                                            "description": "手机号"
                                        },
                                        "region": {
                                            "type": "object",
                                            "properties": {
                                                "regionLevel": {
                                                    "type": "integer",
                                                    "description": "区域级别，0-Province,  1-City, 2-District"
                                                },
                                                "regionId": {
                                                    "type": "integer",
                                                    "description": "区域Id"
                                                }
                                            }
                                        },
                                        "detailAddress": {
                                            "type": "string",
                                            "description": "详细地址"
                                        },
                                        "userName": {
                                            "type": "string",
                                            "description": "用户名称"
                                        }
                                    }
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "查询成功",
                        "schema": {
                            "type": "object",
                            "description": "查询结果",
                            "properties": {
                                "paging": {
                                    "$ref": "definitions/search/ResponsePaging.json"
                                },
                                "itemList": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "addressId": {
                                                "type": "integer",
                                                "description": "地址编号"
                                            },
                                            "recieveName": {
                                                "type": "string",
                                                "description": "收货人姓名"
                                            },
                                            "cellphone": {
                                                "type": "string",
                                                "description": "手机号"
                                            },
                                            "region": {
                                                "$ref": "definitions/region/Region.json"
                                            },
                                            "userId": {
                                                "type": "integer",
                                                "description": "用户编号"
                                            },
                                            "userName": {
                                                "type": "string",
                                                "description": "用户名"
                                            },
                                            "isDefault": {
                                                "type": "boolean",
                                                "description": "是否默认地址"
                                            },
                                            "updateTime": {
                                                "type": "string",
                                                "format": "date",
                                                "description": "更新时间"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
