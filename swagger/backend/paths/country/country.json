{
  "swagger": "2.0",
  "paths": {
    "/country/search": {
      "post": {
        "summary": "一村一档",
        "tags": [ "Country" ],
        "parameters": [
          {
            "$ref": "#/definitions/Token"
          },
          {
            "in": "body",
            "name": "body",
            "description": "分页和查询条件",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "paging": {
                  "$ref": "#/definitions/Paging"
                },
                "condition": {
                  "type": "object",
                  "description": "查询条件",
                  "properties": {
                    "region": {
                      "type": "object",
                      "properties": {
                        "cityCode": {
                          "type": "string",
                          "description": "市州code"
                        },
                        "districtCode": {
                          "type": "string",
                          "description": "区县code"
                        },
                        "townCode": {
                          "type": "string",
                          "description": "乡镇code"
                        }
                      }
                    },
                    "countryName": {
                      "type": "string",
                      "default": "行政村名称"
                    },
                    "createUserName": {
                      "type": "string",
                      "description": "代办员姓名"
                    },
                    "phone": {
                      "type": "string",
                      "description": "联系电话"
                    },
                    "startTime": {
                      "type": "datetime",
                      "default": "时间范围-开始"
                    },
                    "endTime": {
                      "type": "datetime",
                      "default": "时间范围-结束"
                    }
                  }
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "查询成功",
            "schema": {
              "type": "object",
              "properties": {
                "msg": {
                  "type": "string",
                  "description": "成功信息"
                },
                "code": {
                  "type": "string",
                  "description": "成功编码 0",
                  "required": true
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "paging": {
                      "$ref": "#/definitions/ResponsePaging"
                    },
                    "itemList": {
                      "type": "array",
                      "items": {
                        "$ref": "#/definitions/countryItem"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },


    "/country/export": {
      "post": {
        "tags": [ "Country" ],
        "summary": "一村一档导出",
        "parameters": [
          {
            "$ref": "#/definitions/Token"
          },
          {
            "in": "body",
            "name": "body",
            "description": "条件",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "region": {
                  "type": "object",
                  "properties": {
                    "cityCode": {
                      "type": "string",
                      "description": "市州code"
                    },
                    "districtCode": {
                      "type": "string",
                      "description": "区县code"
                    },
                    "townCode": {
                      "type": "string",
                      "description": "乡镇code"
                    }
                  }
                },
                "countryName": {
                  "type": "string",
                  "default": "行政村名称"
                },
                "createUserName": {
                  "type": "string",
                  "description": "代办员姓名"
                },
                "phone": {
                  "type": "string",
                  "description": "联系电话"
                },
                "startTime": {
                  "type": "datetime",
                  "default": "时间范围-开始"
                },
                "endTime": {
                  "type": "datetime",
                  "default": "时间范围-结束"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "返回结果",
            "schema": {
              "type": "object",
              "properties": {
                "msg": {
                  "type": "string",
                  "description": "成功信息"
                },
                "code": {
                  "type": "string",
                  "description": "成功编码 0",
                  "required": true
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "exportUrl": {
                      "type": "string",
                      "description": "导出链接"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }

  }
}