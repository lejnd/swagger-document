{
    "swagger": "2.0",
    "paths": {
        "/supplies/search": {
            "post": {
                "tags": [
                    "Supply"
                ],
                "summary": "查询供应所有信息",
                "description": "列表按照发布时间倒序",
                "parameters": [
                    {
                        "$ref": "definitions/search/token.json"
                    },
                    {
                        "in": "body",
                        "name": "query",
                        "required": true,
                        "description": "查询条件",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "paging": {
                                    "$ref": "definitions/search/Paging.json"
                                },
                                "condition": {
                                    "type": "object",
                                    "description": "查询条件",
                                    "properties": {
                                        "title": {
                                            "type": "string",
                                            "description": "供应商名称"
                                        },
                                        "number": {
                                            "type": "string",
                                            "description": "货品供应编号"
                                        },
                                        "categoryName": {
                                            "type": "string",
                                            "description": "供应品类名称"
                                        },
                                        "publishStartTime": {
                                            "type": "string",
                                            "description": "发布时间起始范围 yyyy-MM-dd HH:mm:ss"
                                        },
                                        "publishEndTime": {
                                            "type": "string",
                                            "description": "发布时间结束范围 yyyy-MM-dd HH:mm:ss"
                                        },
                                        "approveStartTime": {
                                            "type": "string",
                                            "description": "审核时间起始范围 yyyy-MM-dd HH:mm:ss"
                                        },
                                        "approveEndTime": {
                                            "type": "string",
                                            "description": "审核时间结束范围 yyyy-MM-dd HH:mm:ss"
                                        },
                                        "status": {
                                            "type": "integer",
                                            "description": "状态, -1:全部, 0:待审核, 1:进行中, 2:已结束, 3:被驳回 "
                                        }
                                    }
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "返回符合搜索条件的所有信息",
                        "schema": {
                            "type": "object",
                            "description": "获取结果",
                            "properties": {
                                "msg": {
                                    "type": "string",
                                    "description": "获取成功"
                                },
                                "code": {
                                    "type": "string",
                                    "description": "成功编码 0",
                                    "required": true
                                },
                                "data": {
                                    "type": "object",
                                    "properties": {
                                        "pagination": {
                                            "$ref": "definitions/search/ResponsePaging.json"
                                        },
                                        "itemList": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "number": {
                                                        "type": "string",
                                                        "description": "货品供应编号"
                                                    },
                                                    "title": {
                                                        "type": "string",
                                                        "description": "供应商名称"
                                                    },
                                                    "address": {
                                                        "type": "string",
                                                        "description": "指定产地"
                                                    },
                                                    "category": {
                                                        "type": "string",
                                                        "description": "品类"
                                                    },
                                                    "standards": {
                                                        "type": "string",
                                                        "description": "规格要求"
                                                    },
                                                    "cnt": {
                                                        "type": "string",
                                                        "description": "采购数量"
                                                    },
                                                    "price": {
                                                        "type": "string",
                                                        "description": "心里价位"
                                                    },
                                                    "publishTime": {
                                                        "type": "string",
                                                        "description": "发布时间 yyyy-MM-dd HH:mm:ss"
                                                    },
                                                    "approveTime": {
                                                        "type": "string",
                                                        "description": "审核时间 yyyy-MM-dd HH:mm:ss"
                                                    },
                                                    "status": {
                                                        "type": "string",
                                                        "description": "审核状态, -1:全部, 0:待审核, 1:进行中, 2:已结束, 3:被驳回"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/supplies/batch/reject": {
            "post": {
                "tags": [
                    "Supply"
                ],
                "summary": "驳回供应信息",
                "parameters": [
                    {
                        "$ref": "definitions/search/token.json"
                    },
                    {
                        "in": "body",
                        "name": "infoIds",
                        "type": "object",
                        "required": true,
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "description": "信息 ID"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "msg": {
                                    "type": "string",
                                    "description": "成功：已驳回；失败：[供应商名称, ...] 驳回失败"
                                },
                                "code": {
                                    "type": "string",
                                    "description": "成功：0; 失败：1001",
                                    "required": true
                                }
                            }
                        }
                    }
                }
            }
        },
        "/supplies/batch/approval": {
            "post": {
                "tags": [
                    "Supply"
                ],
                "summary": "审核供应信息",
                "parameters": [
                    {
                        "$ref": "definitions/search/token.json"
                    },
                    {
                        "in": "body",
                        "name": "infoIds",
                        "type": "object",
                        "required": true,
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "description": "信息 ID"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "msg": {
                                    "type": "string",
                                    "description": "成功：已审核；失败：[供应商名称, ...] 审核失败"
                                },
                                "code": {
                                    "type": "string",
                                    "description": "成功：0; 失败：1001",
                                    "required": true
                                }
                            }
                        }
                    }
                }
            }
        },
        "/supplies/batch/imports": {
            "post": {
                "tags": [
                    "Supply"
                ],
                "summary": "导入供应信息",
                "parameters": [
                    {
                        "$ref": "definitions/search/token.json"
                    },
                    {
                        "in": "body",
                        "name": "import",
                        "type": "object",
                        "required": true,
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "description": "信息 ID"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "msg": {
                                    "type": "string",
                                    "description": "成功：已导入；失败：[供应商名称, ...] 导入失败"
                                },
                                "code": {
                                    "type": "string",
                                    "description": "成功：0; 失败：1001",
                                    "required": true
                                }
                            }
                        }
                    }
                }
            }
        },
        "/supplies/{infoId}": {
            "get": {
                "tags": [
                    "Supply"
                ],
                "summary": "获取供应信息详情",
                "parameters": [
                    {
                        "$ref": "definitions/search/token.json"
                    },
                    {
                        "in": "path",
                        "type": "string",
                        "required": true,
                        "name": "infoId",
                        "description": "信息 id"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "返回信息详情",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "msg": {
                                    "type": "string",
                                    "description": "获取成功"
                                },
                                "code": {
                                    "type": "string",
                                    "description": "成功：0",
                                    "required": true
                                },
                                "data": {
                                    "$ref": "definitions/goods/supply.json"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
