{
    "definitions": {
        "countryItem": {
            "type": "object",
            "description": "一村一档列表",
            "properties": {        
                "id": {
                    "type": "number",
                    "description": "一村一档键ID"
                },         
                "cityName": {
                    "type": "string",
                    "description": "州/市"
                },          
                "districtName": {
                    "type": "string",
                    "description": "区/县"
                },          
                "townName": {
                    "type": "string",
                    "description": "乡/镇"
                },       
                "villageName": {
                    "type": "string",
                    "description": "村"
                },                              
                "areaNum": {
                    "type": "number",
