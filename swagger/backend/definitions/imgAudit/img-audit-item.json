{
    "type": "object",
    "description": "图片审核记录传输对象",
    "properties": {
        "Id": {
            "type": "string",
            "description": "主键Id"
        },
        "EnterpriseId": {
            "type": "string",
            "description": "企业Id"
        },
        "EnterpriseName": {
            "type": "string",
            "description": "企业名称"
        },
        "OldImgUrl": {
            "type": "string",
            "description": "更新前的图片短链接"
        },
        "NewImgUrl": {
            "type": "string",
            "description": "更新后的图片短链接"
        },
        "ImgType": {
            "type": "number",
            "description": "图片类型：1用户头像，2企业形象"
        },
        "RelatedId": {
            "type": "string",
            "description": "根据图片类型所关联的源表Id"
        },
        "State": {
            "type": "number",
            "description": "状态：0待审核，1已审核，2已驳回"
        },
        "Source": {
            "type": "number",
            "description": "来源：1APP，2PC"
        },
        "UploadTime": {
            "type": "string",
            "description": "图片上传时间"
        },
        "AuditTime": {
            "type": "string",
            "description": "审核时间"
        },
        "UpdateAt": {
            "type": "string",
            "description": "记录更新时间"
        },
        "UpdateId": {
            "type": "string",
            "description": "更新人Id"
        },
        "CreateAt": {
            "type": "string",
            "description": "记录创建时间"
        },
        "CreateId": {
            "type": "string",
            "description": "创建人Id"
        }
    }
}
