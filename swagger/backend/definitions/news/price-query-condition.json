{
    "type": "object",
    "description": "价格播报搜索条件",
    "properties": {
        "paging": {
            "$ref": "../search/Paging.json"
        },
        "sort": {
            "$ref": "../search/Sort.json"
        },
        "condition": {
            "type": "object",
            "properties": {
                "category": {
                    "type": "string",
                    "description": "品类"
                },
                "source": {
                    "type": "string",
                    "description": "价格来源"
                },
                "broadcastStartTime": {
                    "type": "integer",
                    "description": "播报起始时间"
                },
                "broadcastEndTime": {
                    "type": "integer",
                    "description": "播报结束时间"
                },
                "auditStartTime": {
                    "type": "integer",
                    "description": "审核起始时间"
                },
                "auditEndTime": {
                    "type": "integer",
                    "description": "审核结束时间"
                },
                "broadcastMan": {
                    "type": "string",
                    "description": "播报人"
                },
                "auditMan": {
                    "type": "string",
                    "description": "审核人"
                },
                "dataState": {
                    "type": "number",
                    "description": "数据状态，0待审核，1已审核。默认传-1返回所有"
                }
            }
        }
    }
}
