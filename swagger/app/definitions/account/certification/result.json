{
    "swagger": "2.0",
    "definitions": {
        "CertificationResult": {
            "type": "object",
            "properties": {
                "status": {
                    "type": "integer",
                    "description": "审核状态，0: 未认证, 1: 认证中, 2: 认证通过, 3: 认证失败"
                },
                "failureReason": {
                    "type": "string",
                    "description": "若认证失败，此处需返回失败原因"
                },
                "realName": {
                    "type": "string",
                    "description": "若认证通过，此处返回真实姓名"
                },
                "idCardNumber": {
                    "type": "string",
                    "description": "若认证通过，此处返回身份证号码"
                }
            }
        }
    }
}
