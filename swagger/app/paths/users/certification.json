{
    "swagger": "2.0",
    "paths": {
        "/users/certification/result": {
            "post": {
                "tags": ["Users"],
                "summary": "获取实名认证结果",
                "parameters": [{
                    "$ref": "#/parameters/Token"
                }, {
                    "in": "body",
                    "name": "body",
                    "required": true,
                    "type": "object",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "type": {
                                "type": "integer",
                                "description": "类型, 0: 个人认证, 1: 企业认证"
                            }
                        }
                    }
                }],
                "responses": {
                    "200": {
                        "description": "获取成功",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "data": {
                                    "$ref": "#/definitions/CertificationResult"
                                },
                                "code": {
                                    "type": "integer"
                                },
                                "msg": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/users/certification/verify": {
            "post": {
                "tags": ["Users"],
                "summary": "提交实名认证资料",
                "parameters": [{
                    "$ref": "#/parameters/Token"
                }, {
                    "in": "body",
                    "name": "body",
                    "required": true,
                    "type": "object",
                    "schema": {
                        "$ref": "#/definitions/CertificationInfo"
                    }
                }],
                "responses": {
                    "200": {
                        "description": "提交成功"
                    }
                }
            }
        }
    }
}
