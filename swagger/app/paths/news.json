                        "properties": {
                            "paging": {
                                "$ref": "#/definitions/Paging"
                            }
                        }
                    }
                }],
                "responses": {
                    "200": {
                        "description": "获取成功",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "data": {
                                    "type": "array",
                                    "items": {
