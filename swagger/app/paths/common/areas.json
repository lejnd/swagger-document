{
    "swagger": "2.0",
    "paths": {
        "/common/regions/{level}/{id}": {
            "post": {
                "tags": ["Common"],
                "summary": "获取地区数据",
                "parameters": [{
                    "in": "path",
                    "name": "level",
                    "required": true,
                    "type": "integer",
                    "description": "级别, 0: 国家, 1: 省, 2: 市, 3: 区"
                }, {
                    "in": "path",
                    "name": "id",
                    "required": true,
                    "type": "integer",
                    "description": "省、市、区ID, leve=0时返回全国所有的省数据, level=1时返回该id省下所有城市数据"
                }],
                "responses": {
                    "200": {
                        "description": "获取成功",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "data": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "description": "ID"
                                            },
                                            "name": {
                                                "type": "string",
                                                "description": "省市区名"
                                            }
                                        }
                                    }
                                },
                                "code": {
                                    "type": "integer"
                                },
                                "msg": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/common/regions/version": {
            "get": {
                "tags": ["Common"],
                "summary": "获取地区数据版本号",
                "parameters": [
                    {
                        "$ref": "#/parameters/Token"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "获取成功",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "data": {
                                    "type": "string",
                                    "description": "地区版本号"
                                },
                                "code": {
                                    "type": "integer"
                                },
                                "msg": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
