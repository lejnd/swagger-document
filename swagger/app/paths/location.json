{
    "swagger": "2.0",
    "paths": {
        "/location/current": {
            "post": {
                "tags": ["Location"],
                "summary": "获取当前城市",
                "parameters": [{
                    "in": "body",
                    "name": "body",
                    "required": true,
                    "type": "object",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "lat": {
                                "type": "number",
                                "double": "double",
                                "description": "纬度"
                            },
                            "lng": {
                                "type": "number",
                                "double": "double",
                                "description": "经度"
                            }
                        }
                    }
                }],
                "responses": {
                    "200": {
                        "description": "获取成功",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "data": {
                                    "type": "object",
                                    "properties": {
                                        "city": {
                                            "$ref": "#/definitions/City"
                                        }
                                    }
                                },
                                "code": {
                                    "type": "integer"
                                },
                                "msg": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/location/all": {
            "get": {
                "tags": ["Location"],
                "summary": "获取所有位置",
                "parameters": [{
                    "$ref": "#/parameters/Token"
                }],
                "responses": {
                    "200": {
                        "description": "获取成功",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "data": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "firstLetter": {
                                                "type": "string",
                                                "description": "首字母"
                                            },
                                            "cities": {
                                                "type": "array",
                                                "items": {
                                                    "$ref": "#/definitions/City"
                                                }
                                            }
                                        }
                                    }
                                },
                                "code": {
                                    "type": "integer"
                                },
                                "msg": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
